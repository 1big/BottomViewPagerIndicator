<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomBottomPageIndicator">
        <attr name="indicator_spacing" format="dimension"/>
        <attr name="indicator_type"  format="enum">
            <enum name="image" value="0"/>
            <enum name="text" value="1"/>
        </attr>
        <attr name="indicator_icon" format="reference"/>
        <attr name="indicator_textSize" format="dimension"/>
        <attr name="indicator_textColor" format="color"/>
        <attr name="strokeDrawable" format="reference"/>
        <attr name="solidDrawable" format="reference"/>
    </declare-styleable>
</resources>
